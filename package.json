{
  "name": "nestjs-prisma-service",
  "version": "0.0.1",
  "description": "",
  "author": "lwp2333",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "prisma:db-pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint --fix \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.1",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/platform-ws": "^9.4.1",
    "@nestjs/websockets": "^9.4.1",
    "@prisma/client": "^4.14.1",
    "ali-oss": "^6.17.1",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "ejs": "^3.1.9",
    "ffmpeg-stream": "^0.7.0",
    "nanoid": "3.3.4",
    "node-fetch": "2.6.11",
    "progress-stream": "^2.0.0",
    "puppeteer": "^20.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/testing": "^9.4.1",
    "@types/ali-oss": "^6.16.8",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.3",
    "@types/progress-stream": "^2.0.2",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^4.14.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "webpack": "^5.83.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
